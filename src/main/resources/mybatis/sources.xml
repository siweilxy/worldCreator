<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.siwei.worldCreator.Source">
	<insert id="insertSources" useGeneratedKeys="true" keyProperty="id">
		insert into
		sources(x,y,up,down)values(#{x},#{y},#{up},#{down})
	</insert>
	<update id="updateSources">
	</update>
	<delete id="deleteSources">
	</delete>
	<select id="selectAllSources" resultType="Sources">
		select * from sources
	</select>
</mapper>